// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Messages {
  id        String   @id @default(uuid())
  fullname  String
  subject   String
  email     String
  message   String
  isRead    Boolean  @default(false)
  createdAt DateTime
  updatedAt DateTime
}
